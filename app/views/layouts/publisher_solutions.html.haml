-inside_layout 'admin' do
	.navContainer 
		%ul.categories_browser
			%li
				= link_to "solucoes", "javascript://"
				%ul.with_images
					%li.more_link
						= link_to "criar novo", new_admin_solution_path

					-Solution.all.each do |solution|
						%li
							.grid_1.alpha
								= image_tag "/images/noimg_grid1.png"
							.grid_3.rule-two.omega
								= link_to solution.lang_solution.first.name, admin_solution_path(solution)
								
	=yield