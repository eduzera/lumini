-inside_layout 'admin' do
	.navContainer 
		%ul.categories_browser
			%li
				= link_to "designers", "javascript://"
				%ul.with_images
					%li.more_link
						= link_to "criar novo", new_admin_designer_path

					-Designer.all.each do |designer|
						%li
							.grid_1.alpha
								= image_tag designer.img.url(:grid_1)
							.grid_3.rule-two.omega
								= link_to designer.name, admin_designer_path(designer)
								
	=yield